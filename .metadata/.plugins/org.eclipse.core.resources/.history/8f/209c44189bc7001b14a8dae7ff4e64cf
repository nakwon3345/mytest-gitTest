package com.work.model;

public class Concert {
	
	private String concertCode;
	private String concertName;
	private String concertDate;
	private String concertTime;
	private String femaleActor;
	private String maleActor;
	/**
	 * @return the concertCode
	 */
	public String getConcertCode() {
		return concertCode;
	}
	/**
	 * @param concertCode the concertCode to set
	 */
	public void setConcertCode(String concertCode) {
		this.concertCode = concertCode;
	}
	/**
	 * @return the concertName
	 */
	public String getConcertName() {
		return concertName;
	}
	/**
	 * @param concertName the concertName to set
	 */
	public void setConcertName(String concertName) {
		this.concertName = concertName;
	}
	/**
	 * @return the concertDate
	 */
	public String getConcertDate() {
		return concertDate;
	}
	/**
	 * @param concertDate the concertDate to set
	 */
	public void setConcertDate(String concertDate) {
		this.concertDate = concertDate;
	}
	/**
	 * @return the concertTime
	 */
	public String getConcertTime() {
		return concertTime;
	}
	/**
	 * @param concertTime the concertTime to set
	 */
	public void setConcertTime(String concertTime) {
		this.concertTime = concertTime;
	}
	/**
	 * @return the femaleActor
	 */
	public String getFemaleActor() {
		return femaleActor;
	}
	/**
	 * @param femaleActor the femaleActor to set
	 */
	public void setFemaleActor(String femaleActor) {
		this.femaleActor = femaleActor;
	}
	/**
	 * @return the maleActor
	 */
	public String getMaleActor() {
		return maleActor;
	}
	/**
	 * @param maleActor the maleActor to set
	 */
	public void setMaleActor(String maleActor) {
		this.maleActor = maleActor;
	}
	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append(concertCode);
		builder.append(", ");
		builder.append(concertName);
		builder.append(", ");
		builder.append(concertDate);
		builder.append(", ");
		builder.append(concertTime);
		builder.append(", ");
		builder.append(femaleActor);
		builder.append(", ");
		builder.append(maleActor);
		return builder.toString();
	}

	
}
